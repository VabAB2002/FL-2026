# SEC 10-K Data Pipeline Environment Variables
# =============================================
# Copy this file to .env and fill in your values
# DO NOT COMMIT .env to version control!

# SEC Edgar API (Required by SEC)
# Format: "CompanyName ContactEmail"
SEC_API_USER_AGENT="YourCompanyName your.email@example.com"

# AWS Credentials (for S3 backups)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1
S3_BUCKET_NAME=finloom-sec-data

# Processing Configuration
MAX_WORKERS=4
RATE_LIMIT_PER_SEC=8

# Optional: Override config paths
# CONFIG_PATH=config/settings.yaml
# DATABASE_PATH=data/database/finloom.duckdb
