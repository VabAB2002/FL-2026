# SEC 10-K Data Pipeline Environment Variables
# =============================================
# Copy this file to .env and fill in your values
# DO NOT COMMIT .env to version control!

# Environment (development, staging, production)
FINLOOM_ENV=development

# SEC Edgar API (Required by SEC)
# Format: "CompanyName ContactEmail"
SEC_API_USER_AGENT="FinLoom contact@example.com"

# AWS Credentials (for S3 backups - required in production)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_DEFAULT_REGION=us-east-1
# S3_BUCKET_NAME=finloom-sec-data

# Neo4j Graph Database
NEO4J_PASSWORD=finloom123
# NEO4J_URI=bolt://localhost:7687
# NEO4J_USER=neo4j
# NEO4J_DATABASE=neo4j

# Processing Configuration
# MAX_WORKERS=4
# RATE_LIMIT_PER_SEC=8

# Optional: Override config paths
# FINLOOM_DB_PATH=data/database/finloom.duckdb
# FINLOOM_LOG_LEVEL=INFO
# FINLOOM_S3_BUCKET=finloom-sec-data
# FINLOOM_S3_REGION=us-east-1
